{
  "name": "web-starter-kit",
  "version": "1.0.0",
  "description": "IG Web Starter Kit",
  "private": true,
  "directories": {
    "test": "tests"
  },
  "engines": {
    "node": "^10.0.0",
    "npm": "3.10.10"
  },
  "scripts": {
    "build:server": "cross-env node node_modules/webpack/bin/webpack.js --config config/webpack.server.config.js",
    "build:client": "cross-env node node_modules/webpack/bin/webpack.js --config config/webpack.client.config.js",
    "build": "yarn build:server && yarn build:client",
    "start": "node ./dist-server/server.js",
    "dev:client": "cross-env webpack --progress --watch --config ./config/webpack.client.config.js",
    "dev:server": "cross-env nodemon -r tsconfig-paths/register -e js,ts,tsx --inspect=9229 server/app.ts --exec ts-node",
    "test": "yarn test:server && yarn test:client",
    "test:client": "NODE_ENV=test mocha --exit --opts ./mocha.opts \"./client/**/*.test.ts*\"",
    "test:server": "NODE_ENV=test mocha --exit --opts ./mocha.opts server/test-helpers/setup-database-for-testing.ts \"./server/**/*.test.ts*\"",
    "ts": "yarn test:server",
    "lint": "tslint --project ./tsconfig.json -c ./tslint.json './**/*.ts*' -e './node_modules/**' -e './dist/**' -e './client/**' -e './server/**'",
    "deploy": "sh utilities/deployer.sh msl-dashboard",
    "heroku-postbuild": "npm run build",
    "migrate:up": "knex migrate:latest",
    "migrate:down": "knex migrate:rollback"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.0.5",
    "css-loader": "^3.2.0",
    "mini-css-extract-plugin": "^0.8.0",
    "style-loader": "^1.0.0",
    "typescript": "^3.0.0",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.9"
  },
